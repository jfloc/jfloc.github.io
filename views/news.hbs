<header>
    {{> navbar}}
</header>

<main>
    <div class="filter_bar">
        <button>Filter</button>
    </div>
    <div class="travel_news">
        {{#each countries}}
        <article>
            <h2>{{name}}</h2>
        </article>
        {{/each}}
    </div>
</main>
